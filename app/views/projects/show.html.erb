<% provide(:title, 'Project Details') %>
<h2>Project Details</h2>
<div class="row">
  <div class="span6">
    <p id="notice"> <%= notice %> </p>
 
    <h3>
      <!--<b>Title:</b> Use this instead for header-->
      <%= @project.title %>
    </h3>
    
    <p>
      <b>Contributors:</b>
      <%= @project.contributor%>
    </p>

    <p>
      <b>Description</b>
      <%= @project.description %>
    </p>

    <hr>

    <p>
      <b>Tags:</b>
      <%= @project.tag %>
    </p>
 
    <p>
      <b>Link:</b>
      <%= @project.link %>
    </p>

    <p>
      <b>Link Type:</b>
      <%= @project.link_type %>
    </p>

    <p>
      <b>Rating:</b>
      <%= @project.rating %>
    </p>
    
</div>
